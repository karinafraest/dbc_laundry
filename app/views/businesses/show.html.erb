<h1>All Admins</h1>
<table style="width:30%">
  <tr>
    <th>Admin</th>
    <th>Email</th> 
  </tr>
  <% current_admin.business.admins.each do |admin| %>
  <tr>
    <td><%= admin.name %></td>
    <td><%= admin.email %></td> 
  </tr>
  <% end %>
</table>
<br>
<br>
<h1>All Shippers</h1>
<table style="width:50%">
  <tr>
    <th>Name</th> 
    <th>Email</th> 
    <th>Phone</th>
  </tr>
  <% current_admin.business.shippers.each do |shipper| %>
  <tr>
    <td><%= shipper.name %></td> 
    <td><%= shipper.email %></td> 
    <td><%= shipper.phone %></td>
  </tr>
  <% end %>
</table>
<br>
<br>
<h1>All Clients</h1>
<table style="width:100%">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Order count</th> 
    <th>Total $</th>
  </tr>
  <% current_admin.business.clients.each do |client| %>
  <tr>
    <td><%= client.name %></td>
    <td><%= client.email %></td>
    <td><%= client.phone %></td>
    <td><%= client.orders.count %></td> 
    <td><%= client.orders.sum(:total) %></td>
  </tr>
  <% end %>
</table>
<br>

<div class="col">
   <%= render partial: "boxes/index"%>
</div>